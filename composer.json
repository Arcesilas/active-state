{
    "name": "arcesilas/active-state",
    "description": "Laravel link active state helper based on URL, route or query string",
    "keywords": ["laravel", "active", "link", "menu", "route", "url", "query", "request"],
    "homepage": "https://github.com/Arcesilas/active-state",
    "authors": [
        {
            "name": "Pyae Sone Nyein",
            "email": "promise286@gmail.com"
        },
        {
            "name": "Olivier Cecillon",
            "email": "arcesilas@neutre.email"
        }
    ],
    "require": {
        "laravel/framework": "^6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^8",
        "orchestra/testbench": "^4"
    },
 	"autoload": {
	    "psr-4": {
	        "Arcesilas\\ActiveState\\": "src/"
	    },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcesilas\\ActiveState\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Arcesilas\\ActiveState\\ActiveStateServiceProvider"
            ],
            "aliases": {
                "Active": "Arcesilas\\ActiveState\\ActiveFacade"
            }
        },
        "branch-alias": {
            "dev-develop-v4": "4.0.x-dev"
        }
    }
}
